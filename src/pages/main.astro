<style>
    .unavailable {
        @apply grayscale;
    }
    .events-hover {
        @apply md:-rotate-[0deg] md:translate-y-[0%] transition-transform delay-200 duration-300 translate-y-[2%] group-hover:translate-y-[2%] w-[33%];
    }
    .harsh-shadow {
        filter: drop-shadow(5px 10px 0rem rgba(0, 0, 0, 0.301));
    }
    #mascot-selectable:focus #mascot-move {
        @apply ease-in-out duration-300 opacity-0 transform translate-y-64;
    }
    #mascot-move {
        @apply ease-in-out duration-300;
    }
    .tape-down {
        @apply ease-in-out duration-500 translate-y-0;
    }

    /* Targeting with :hover here on the wrapper to make hovering more seamless*/
    #tape:hover .tape-wrapper {
        @apply translate-y-0 rotate-0;
    }

    .cover {
        transform-style: preserve-3d;
        transition: transform 0.5s ease-in-out;
        transform-origin: 50% calc(90%);
    }

    #tape:hover .cover {
        transform: rotateX(-180deg);
    }

    .corkboard-item {
        @apply harsh-shadow transition-transform duration-500 hover:rotate-0 hover:z-50 md:-mx-1 md:-my-1 /*bg-red-800*/;
    }

    .corkboard-static {
        @apply hidden lg:block md:-mx-3 md:-my-3;
    }

    @media (max-width: 767px) { /* Tailwind's medium */
        .corkboard-item {
            grid-column: 1 / -1 !important; /* Forces items to span full width */
            width: 80%;
            margin-left: auto;
            margin-right:auto;
        }
    }


</style>
