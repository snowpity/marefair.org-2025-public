---
import { Image, getImage } from "astro:assets";
import { getEntry } from 'astro:content';
import type {Sponsor, SponsorCollection} from '@data/Sponsor';

import Layout from '@layouts/Layout.astro';
import Banner from "@components/Banner.astro";

// SCRAPBOOK
import scrapbook_header from "@assets/gallery/scrapbook.png";

import titleLogo from "@assets/corkboard/sign_frame.png";
import titleNeon from "@assets/corkboard/sign_neon.png";

import corkboardRaw from "@assets/corkboard/corktile.png";
import frameTop from "@assets/corkboard/frametop.png";
import frameSidesRaw from "@assets/corkboard/framesides.avif";
import frameBottom from "@assets/corkboard/framebottom.png";
import frameTopTrinket from "@assets/corkboard/frametop_stuff3.png";
import frameTopTrinket_mobile from "@assets/corkboard/frametop_stuff3_mobile.png";
import frameBottomTrinket from "@assets/corkboard/framebottom_stuff4.png";
import frameBottomTrinket_mobile from "@assets/corkboard/framebottom_stuff4_mobile.png";

import about from "@assets/corkboard/About_us.png";
import news from "@assets/corkboard/News_Link.png";
import contact from "@assets/contact/contact_full.png";
import contact_phone from "@assets/contact/Mare_Fair_3_Contacts_phone.png";
import conduct from "@assets/corkboard/coc.png";
import venue from "@assets/corkboard/Venue_Poster_Mare_Fair_2025_pinned.png"
import apply from "@assets/corkboard/apply_flyer_pinned.png"
import registration from "@assets/corkboard/2025_Registration_Button_pinned.png"
import soon from "@assets/corkboard/coming_soon.png"
import mascots from "@assets/corkboard/OurMascotsButton.png"
import mascots1 from "@assets/corkboard/OurMascots_MoveableStrip.png"
import events from "@assets/corkboard/events/events.png"
import cinema from "@assets/events/brochure_cinemare.png"
import cruise from "@assets/events/brochure_cruise.png"
import poniinspace from "@assets/corkboard/events/poniinspace.png"
import conbook from "@assets/conbook/cover.png"

/*
import FairFlyerPin from "@assets/corkboard/FairFlyerPin.png"
import SoireePin from "@assets/corkboard/SoireePin.png"
import SwirlyMimosa from "@assets/corkboard/Swirly_Mimosa.png"
*/
import Credit from "@assets/corkboard/TV.png"

import music from "@assets/corkboard/1MusicianCassetteClosed.png"
import music2 from "@assets/corkboard/4MusicianCassetteBottom.png"
import music3 from "@assets/corkboard/5MusicianCassetteTop.png"

import schedule from "@assets/schedules/schedule.png"
import map from "@assets/map-venue/map.png"
import vendor from "@assets/vendors/YardSard.png"
import tendieFrenzy from "@assets/misc/TendieFrenzy.png"

const frameSides = await getImage({ src: frameSidesRaw, format:"avif" });
const corkboard = await getImage({ src: corkboardRaw, format:"avif" });

import sponsorTop from "@assets/sponsor/sponsor_top.png";
import sponsorMiddleRaw from "@assets/sponsor/sponsor_middle.png";
import sponsorBottom from "@assets/sponsor/sponsor_bottom.png";
import gigachadRaw from "@assets/sponsor/gigachad.png";
const sponsorMiddle = await getImage({ src: sponsorMiddleRaw, format:"avif", width: sponsorBottom.width / 2});
const gigachad = await getImage({ src: gigachadRaw, format:"avif", width: sponsorBottom.width / 3 });


import soireeImage from "@assets/pages/news/soiree.png";

const sponsorNameExtractor = (sponsors : SponsorCollection|null|undefined) => {
    return sponsors?.sponsors.map((s: Sponsor) => s.sponsorName) ?? []
}

const sponsorsMareSchizo = sponsorNameExtractor((await getEntry('sponsors', 'Mare Schizo'))?.data)
const sponsorsMareFanatic = sponsorNameExtractor((await getEntry('sponsors', 'Mare Fanatic'))?.data)
const sponsorsMareAficionado = sponsorNameExtractor((await getEntry('sponsors', 'Mare Aficionado'))?.data)
const sponsorsMareConnoisseur = sponsorNameExtractor((await getEntry('sponsors', 'Mare Connoisseur'))?.data)
const sponsorsMareEnthusiast = sponsorNameExtractor((await getEntry('sponsors', 'Mare Enthusiast'))?.data)
const sponsorsMareAdmirer = sponsorNameExtractor((await getEntry('sponsors', 'Mare Admirer'))?.data)
const sponsorsMareEnjoyer = sponsorNameExtractor((await getEntry('sponsors', 'Mare Enjoyer'))?.data)
const sponsorsMareSupporter = sponsorNameExtractor((await getEntry('sponsors', 'Mare Supporter'))?.data)

---
<Layout>
    <Banner/>

    <main class="">
        {/* Weirdest shit, but it needs relative to hide overflow correctly */}
        <div class="relative overflow-hidden w-full h-auto flex justify-center items-center w-screen h-auto md:-mb-[17rem] -mb-[35%]">
            <Image src={titleLogo} format="avif" alt="Mare Fair 2025, September 5 through 7 in Orlando, Florida" class="max-w-none w-[220vw] md:w-auto" width={1700}/>
            <Image src={titleNeon} format="avif" alt="Mare Fair 2025, September 5 through 7 in Orlando, Florida" class="absolute top-0 max-w-none w-[220vw] md:w-auto neon-glow" width={1700}/>
        </div>

        <div class="relative">
            {/* If we have another tiling layer on top of vignette layer, this might be an approach
            <div id="corkboard"       class="absolute max-w-full mx-auto bg-center inset-0 m-auto h-[calc(100% - 25rem)] bg-cover" style={`background-image: url(${backdrop.src}), url(${corkboard.src}); width:calc(${frameSides.width}px - 150px); height: calc(100% - 150px); background-size: 200% ,400px; box-shadow: inset 0 0 200px #803722;`}> </div>
            <div id="corkboard-layer" class="absolute max-w-full mx-auto bg-center inset-0 m-auto h-[calc(100% - 25rem)] bg-cover" style={`background-image: url(${backdrop.src}); width:calc(${frameSides.width}px - 150px); height: calc(100% - 150px); background-size: 170%;`}></div>
            */}
            <div id="board" class="relative h-full overflow-hidden">
                <div id="top" class="relative flex flex-wrap justify-center pointer-events-none">
                    <Image src={frameTop} alt="" format="avif" width={frameTop.width} height={frameTop.height} class=`block mx-auto` style=`min-width: ${frameTop.width}px`/>
                    <Image src={frameTopTrinket} format="avif" alt="" class="absolute sm:block hidden mx-auto z-10 translate-y-7" width={frameTopTrinket.width} style=`min-width: ${frameTopTrinket.width}px`/>
                    <Image src={frameTopTrinket_mobile} format="avif" alt="" class="absolute sm:hidden block mx-auto z-10 translate-y-7" width={frameTopTrinket.width} style=`min-width: ${frameTopTrinket.width}px`/>
                </div>
                {/* change height to Auto if using height affecting divs i.e. texts or relative images*/}
                <div id="cen" class="relative max-w-full mx-auto bg-top flex flex-wrap items-center justify-center px-0 h-auto" style={`background-image: url(${frameSides.src});width: ${frameSidesRaw.width}px;`}>
                    <div id="corkboard" class="absolute max-w-full mx-auto bg-center inset-0 bg-cover -z-10 -mt-[20rem]" style={`background-image: url(${corkboard.src}); width:calc(${frameSidesRaw.width}px - 150px); height: calc(${20}rem + 100%); background-size: 100%; box-shadow: inset 0 0 200px #803722;`}> </div>
                    <div id="corkboard-container" class="flex flex-col md:grid gap-2.5 relative justify-center -translate-y-56 -mb-44 lg:w-[1000px] w-[720px]
                                                        md:grid-cols-[repeat(auto-fit,_50px)] md:auto-rows-[50px] z-[1]" style="grid-auto-flow: row dense;">

                        <a href="/schedule" class="corkboard-item origin-top -rotate-[1deg] translate-y-0 sm:translate-y-0 z-[0]"
                            style="grid-row: span 9 / span 9; grid-column: span 6 / span 6;">
                            <Image src={schedule} transition:name="schedules" format="avif" width="600" alt="Schedules"  class="w-full h-auto">
                        </a>
                        <a href="/venue" class="col-span-5 row-span-7 corkboard-item origin-top rotate-[1deg] translate-y-0 sm:-translate-y-3 -translate-x-0 z-[1]">
                            <Image src={venue} transition:name="venue" format="avif" width="600" alt="Venue" class="w-full h-auto" />
                        </a>

                        <a href="/map" class="col-span-5 row-span-5 corkboard-item origin-top -rotate-[1deg] translate-y-0 sm:-translate-y-3 -translate-x-0 z-[1]">
                            <Image src={map} transition:name="map" format="avif" width="600" alt="Venue Map" class="w-full h-auto">
                        </a>

                        <a href="/vendors" class="col-span-5 row-span-5 corkboard-item origin-top -rotate-[1deg] translate-y-0 sm:-translate-y-3 -translate-x-0 z-[1]">
                            <Image src={vendor} transition:name="vendor" format="avif" width="600" alt="Vendors" class="w-full h-auto">
                        </a>

                        <a href="/register" class="col-span-6 row-span-3 corkboard-item origin-top rotate-[5deg] translate-y-0 sm:translate-y-0 z-[0]">
                            <Image src={registration} transition:name="register" format="avif" width="600" alt="Register" class="w-full h-auto" />
                        </a>

                        <a href="/musician" class="sm:col-span-5 row-span-5 col-span-6 corkboard-item origin-center translate-y-0 sm:translate-y-3 translate-x-0 z-[1]"  id="tape" style="perspective: 1000px;">
                            <div class="rotate-90 translate-y-10 tape-wrapper transition-transform duration-500">
                                <Image src={music3} transition:name="music3" format="avif" width="400" alt="Concert" class="w-full absolute tape-down" />
                                <Image src={music2} transition:name="music2" format="avif" width="400" alt="Concert" class="w-full cover absolute tape-down" />
                                <Image src={music} transition:name="music1" format="avif" width="400" alt="Concert" class="w-full cover relative backface-hidden tape-down" />
                            </div>
                        </a>
                        {/* Reserving space for the cassete, only activate on low */}
                        <div class="w-full h-28 relative md:hidden block invisible" ></div>

                        <a href="/news" class="col-span-6 row-span-7 corkboard-item origin-center -rotate-[3deg] translate-y-0 lg:translate-x-0 z-[0]">
                            <Image src={news} transition:name="newspaper" format="avif" width="600" alt="News" class="w-full h-auto" />
                            <Image src={soireeImage} transition:name="news" format="avif" width="100" alt="" class="absolute top-[27%] right-[10%]" />
                        </a>

                        <a href="/apply" class="col-span-5 row-span-7 corkboard-item origin-top rotate-[-2deg] translate-y-0 sm:translate-y-1 -translate-x-0 z-[1]">
                            <Image src={apply} transition:name="apply" format="avif" width="600" alt="Apply" class="w-full h-auto" />
                        </a>

                        <a href="/events" class="col-span-6 row-span-4 corkboard-item origin-top rotate-[-2deg] translate-y-0 sm:translate-y-0 md:-translate-x-5 z-[2] group">
                            <div class="h-auto flex flex-row items-center justify-between">
                                <Image src={cruise} transition:name="cruise" format="avif" width="100" alt="" class="events-hover md:translate-x-[90%] translate-x-[10%] -rotate-[5deg] group-hover:translate-x-[10%] group-hover:-rotate-[5deg]" />
                                <Image src={poniinspace} transition:name="charity" format="avif" width="100" alt="" class="relative w-[33%]" />
                                <Image src={cinema} transition:name="cinema" format="avif" width="100" alt="" class="events-hover md:-translate-x-[90%] -translate-x-[10%] rotate-[5deg] group-hover:-translate-x-[10%] group-hover:rotate-[5deg]" />
                                <Image src={events} format="avif" width="150" alt="Events" class="absolute left-1/2 -translate-x-1/2 -top-[5%] w-[50%] z-10" />
                            </div>
                        </a>

                        <a href="/about" class="col-span-4 row-span-4 corkboard-item origin-top rotate-[1deg] translate-y-0 sm:-translate-y-2 z-[1]">
                            <Image src={about} transition:name="about" format="avif" width="600" alt="About" class="w-full h-auto" id="about"/>
                        </a>

                        <a href="/contact" class="lg:hidden block col-span-6 row-span-5 corkboard-item origin-top rotate-[-5deg] translate-y-0 sm:translate-y-0 md:-translate-x-5 z-[2]">
                            <div class="h-auto flex flex-row items-center">
                                <div transition:name="phoneScreen"> </div>
                                <Image src={contact_phone} transition:name="phone" format="avif" width="300" alt="" class="w-2/5 -rotate-[5deg] translate-x-2"/>
                                <Image src={contact} transition:name="contact" format="avif" width="400" alt="Contact Us" class="w-3/5" />
                            </div>
                        </a>

                        <a href="/contact" class="lg:block hidden col-span-6 row-span-6 corkboard-item origin-top rotate-[-5deg] translate-y-0 sm:translate-y-0 md:-translate-x-5 z-[2]">
                            <div class="h-auto flex flex-row items-center">
                                <div transition:name="phoneScreen"> </div>
                                <Image src={contact_phone} transition:name="phone" format="avif" width="300" alt="" class="w-2/5 -rotate-[5deg] translate-x-2"/>
                                <Image src={contact} transition:name="contact" format="avif" width="400" alt="Contact Us" class="w-3/5" />
                            </div>
                        </a>

                        <a id="mascot-selectable" href="/mascots" class="col-span-6 row-span-6 corkboard-item origin-top rotate-[3deg] translate-y-0 sm:translate-y-0 translate-x-0 z-[2]">
                            <Image id="mascot-move" src={mascots1} format="avif" width="400" alt="" class="w-full h-auto absolute pointer-events-none" />
                            <Image src={mascots} format="avif" width="400" alt="Our Mascots" class="w-full h-auto" />
                        </a>

                        <a href="https://fair-filer.marefair.org/2025/conbook/MareFair2025_ConBook_Digital.pdf" class="col-span-5 row-span-8 corkboard-item origin-top rotate-[-1deg] translate-y-0 z-[1] hover:!rotate-0">
                            <Image src={conbook} transition:name="conbook" format="avif" width="600" alt="Code of Conduct" class="w-full h-auto pointer-events-none">
                        </a>

                        <a href="/coc" class="col-span-4 row-span-4 corkboard-item origin-center rotate-[-1deg] sm-translate-y-10 translate-y-0 z-[2] hover:!rotate-12">
                            <Image src={conduct} transition:name="coc" format="avif" width="600" alt="Code of Conduct" class="w-full h-auto sm:-translate-y-10 pointer-events-none md:scale-125 scale-100">
                        </a>

                        <a href="/scrapbook" class="col-span-6 row-span-3 corkboard-item origin-top rotate-[-3deg] translate-y-0 sm:translate-y-2 z-[1]">
                            <Image src={scrapbook_header} transition:name="scrapbook" format="avif" width="600" alt="Scrapbook" class="w-full h-auto" id="about"/>
                        </a>

                        <a href="/tendiefrenzy" class="lg:col-span-4 lg:row-span-5 col-span-5 row-span-6 corkboard-item origin-top rotate-[-1deg] translate-y-0 z-[1] hover:!rotate-0">
                            <Image src={tendieFrenzy} transition:name="tf" format="avif" width="600" alt="Code of Conduct" class="w-full h-auto pointer-events-none">
                        </a>

                        {/*
                        <div id="SoireePin" class="corkboard-static col-span-1 row-span-1"> <Image src={SoireePin} format="avif" width="200" alt="" class="w-full h-auto" /> </div>
                        <div id="FairFlyerPin" class="corkboard-static col-span-1 row-span-1"> <Image src={FairFlyerPin} format="avif" width="200" alt="" class="w-full h-auto translate-y-10" /> </div>
                        <div id="MimosaPin" class="corkboard-static col-span-2 row-span-2"> <Image src={SwirlyMimosa} format="avif" width="200" alt="" class="w-full h-auto translate-y-20" /> </div>
                        <div id="spacing" class="col-span-2 row-span-2"></div>
                         */}

                        {/*
                        <div id="soon" class="lg:col-span-4 col-span-3 lg:row-span-4 row-span-3 corkboard-item origin-top rotate-[6deg] translate-y-0 z-[2] hover:!rotate-0 lg:block hidden">
                            <Image src={soon} transition:name="soon" format="avif" width="600" alt="More coming soon" class="w-full h-auto sm:-translate-y-0 pointer-events-none" />
                        </div>
                        */}

                    </div>

                </div>
                <div id="bot" class="relative flex flex-wrap justify-center pointer-events-none z-[0] bottom-64 w-full ">
                    <Image src={frameBottom} alt="" format="avif" width={frameBottom.width} height={frameBottom.height} class=`block mx-auto` style=`min-width: ${frameBottom.width}px`/>
                    <Image src={frameBottomTrinket} format="avif" alt="" class="absolute sm:block hidden block bottom-0 mx-auto z-10 translate-y-[10.5rem]" width={frameBottomTrinket.width} style=`min-width: ${frameBottomTrinket.width}px`/>
                    <Image src={frameBottomTrinket_mobile} format="avif" alt="" class="absolute sm:hidden block block bottom-0 mx-auto z-10 translate-y-[10.5rem]" width={frameBottomTrinket.width} style=`min-width: ${frameBottomTrinket.width}px`/>
                </div>
            </div>

            <div class="2xl:sticky relative flex md:right-0 bottom-0 z-20 scroll-container pointer-events-none md:mt-[-15rem] mt-[-5rem] harsh-shadow">
                <a href="/credits" class="credit-link 2xl:mr-0 mr-auto ml-auto pointer-events-auto">
                  <Image src={Credit} width={400} alt="Credits" class="2xl:md:translate-x-10 hover:translate-x-0 hover:scale-105 transition duration-150 ease-in-out" />
                </a>
            </div>

            {/* Plaque */}
            <div id="plaque" class="relative overflow-x-hidden h-full">
                <div id="top-plaque" class="relative flex flex-wrap justify-center pointer-events-none">
                    <Image src={sponsorTop} alt="" format="avif" width={sponsorTop.width / 2} height={sponsorTop.height / 2} class=`block mx-auto` style=`min-width: ${sponsorTop.width / 2}px`/>
                </div>
                {/* change height to Auto if using height affecting divs i.e. texts or relative images*/}
                <div id="cen-plaque" class="relative max-w-full mx-auto bg-top flex flex-wrap items-center justify-center px-0 h-auto reddit-sans" style={`background-image: url(${sponsorMiddle.src});width: ${sponsorTop.width / 2}px; `}>
                    <div class="absolute -mt-[5rem] w-[550px] bg-fixed bg-no-repeat bg-contain bg-center pointer-events-none opacity-20 z-10" style=`background-image: url(${gigachad.src}); height: calc(100% + 6rem) `> </div>
                    {/* CHANGE -mt and -mb here to make the top and bottom of the plaque not space out */}
                    <div class="relative -mt-20 sm:w-[70%] w-[90%] flex flex-col mx-auto rounded-lg h-auto p-2 text-white text-center items-center justify-center z-10 opacity-90 z-20">
